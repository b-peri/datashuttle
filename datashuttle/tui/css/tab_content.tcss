#FileTree {
    dock: left;
    width: 35%;
    padding: 1 2 1 2;
    margin: 0 4 0 0;
}
#create > Label {
    width: 100%;
    margin: 1 0 1 1;
    text-style: bold;
}
#create > #sub_label {
    margin: 0 0 1 1;
}
#create > TypeBox {
    layout: grid;
    grid-size: 2 2;
    grid-columns: 25;
}
TypeBox > Checkbox {
    width: 18;
    content-align: left middle;
}
#make_folder {
    margin: 1 0 0 0;
}
